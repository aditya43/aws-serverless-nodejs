{
    "StartAt": "GetFileType",
    "States": {
        "GetFileType": {
            "Type": "Pass",
            "Next": "CheckFileType"
        },
        "CheckFileType": {
            "Type": "Choice",
            "Choices": [
                {
                    "Variable": "$.results.fileType",
                    "StringEquals": "jpg",
                    "Next": "ProcessFile"
                }
            ],
            "Default": "DeleteSourceFile"
        },
        "DeleteSourceFile": {
            "Type": "Pass",
            "End": true
        },
        "ProcessFile": {
            "Type": "Parallel",
            "Next": "WriteToDynamoDB",
            "Branches": []
        },
        "WriteToDynamoDB": {
            "Type": "Pass",
            "Next": "DeleteSourceFile"
        }
    }
}